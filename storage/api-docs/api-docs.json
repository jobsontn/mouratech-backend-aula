                                    },
                                    "fatherName": {
                                        "type": "string"
                                    },
                                    "birthday": {
                                        "type": "string"
                                    },
                                    "gender": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string"
                                    },
                                    "cellphone": {
                                        "type": "string"
                                    },
                                    "githubProfile": {
                                        "type": "string"
                                    },
                                    "campusName": {
                                        "type": "string"
                                    },
                                    "courseName": {
                                        "type": "string"
                                    },
                                    "currentPeriod": {
                                        "type": "integer"
                                    },
                                    "group": {
                                        "type": "integer"
                                    }
