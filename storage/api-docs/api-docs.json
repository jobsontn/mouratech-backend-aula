                "operationId": "7c586e93df821205d2c5af51e9dfd6d2",
                "parameters": [
                    {
                        "name": "page",
                        "in": "query",
                        "description": "Número da página",
                        "required": true,
                        "style": "form",
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Lista de estudantes."
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            },
            "post": {
                "tags": [
                    "/students"
                ],
                "summary": "Cria um estudante.",
                "description": "Cria um novo estudante e persiste no banco de dados.",
                "operationId": "9e62661a4847ad2a4c15519a535ad5aa",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "enrollment": {
                                        "type": "string"
                                    },
                                    "CPF": {
                                        "type": "string"
                                    },
                                    "name": {
                                        "type": "string"
                                    },
                                    "motherName": {
                                        "type": "string"
                                    },
                                    "fatherName": {
                                        "type": "string"
                                    },
                                    "birthday": {
                                        "type": "string"
                                    },
                                    "gender": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string"
                                    },
                                    "cellphone": {
                                        "type": "string"
                                    },
                                    "githubProfile": {
                                        "type": "string"
                                    },
                                    "campusName": {
                                        "type": "string"
                                    },
                                    "courseName": {
                                        "type": "string"
                                    },
                                    "currentPeriod": {
                                        "type": "integer"
                                    },
                                    "group": {
                                        "type": "integer"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Novo estudante criado."
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
