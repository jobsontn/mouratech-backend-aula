                    {
                        "name": "id",
                        "in": "path",
                        "description": "Id do estudante",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "Estudante excluído."
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
